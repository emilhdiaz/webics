@CHARSET "ISO-8859-1";

table.Webics {
	width: 98%;
	margin: 1%;
	font-family: Verdana;
}

table.Webics caption {
	font-size: 0.9em;	
}

table[context=Tabulation].Webics thead[context=Tabulation\.Thead] th {
	margin: 1px;
	padding: 3px;
	color: #336699;
	font-weight: bold;
	text-align: center;
	font-weight: normal;
	letter-spacing: 0.05em;
	border-bottom: 2px solid #336699;
}

table[context=Tabulation].Webics tbody[context=Tabulation\.Tbody] td {
	color: #444444;
	padding: 5px 0;
	border-bottom: 1px solid #BBBBBB;
	text-align: center;
	cursor: pointer;
}

table[context=Tabulation].Webics tbody[context=Tabulation\.Tbody] tr:hover {
	background-color: #DDEEFF;
}

table[context=Tabulation].Webics tbody tr.odd {
	background-color: #EEEEEE;
}

table[context=Tabulation].Webics tbody tr.even {
}

table[context=Roster].Webics td {
	text-align: left;
	color: #444444;
}

table[context=Roster].Webics td:first-child {
	color: #336699;
	font-weight: bold;
	padding: 0 0 0 5px;
	width: 35%;
	height: 20px;
}


